#!/bin/bash

Reset="\033[0m"

Black="\033[0;30m"
BBlack="\033[1;32m"
BBBlack="\033[1;33m"
BBBBlack="\033[1;30m"
White="\033[0;37m"
Red="\033[0;31m"
Green="\033[0;32m"
Yellow="\033[0;33m"
Blue="\033[0;34m"
Purple="\033[0;35m"
Cyan="\033[0;36m"
Orange="\033[1;31m"

Success="$Green"
Warning="$Orange"
Error="$Red"

